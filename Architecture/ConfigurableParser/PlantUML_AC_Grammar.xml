<?xml version='1.0' ?>
<context_free_grammar>
	<start_symbol>PLANTUML_ACTIVITY_DIAGRAM</start_symbol>
	<special_identifiers>
		<special_identifier>startuml</special_identifier>
		<special_identifier>enduml</special_identifier>
		<special_identifier>start</special_identifier>
		<special_identifier>stop</special_identifier>
		<special_identifier>if</special_identifier>
		<special_identifier>then</special_identifier>
		<special_identifier>else</special_identifier>
		<special_identifier>endif</special_identifier>
	</special_identifiers>
	<terminal_symbols>
		<terminal_symbol>comment</terminal_symbol>
		<terminal_symbol>decimalnumber</terminal_symbol>
		<terminal_symbol>integernumber</terminal_symbol>
		<terminal_symbol>identifier</terminal_symbol>
		<terminal_symbol>colon</terminal_symbol>
		<terminal_symbol>semicolon</terminal_symbol>
		<terminal_symbol>twodots</terminal_symbol>
		<terminal_symbol>openbraket</terminal_symbol>
		<terminal_symbol>closebraket</terminal_symbol>
		<terminal_symbol>openparentesis</terminal_symbol>
		<terminal_symbol>closeparentesis</terminal_symbol>
		<terminal_symbol>pipe</terminal_symbol>
		<terminal_symbol>inverteddiagonal</terminal_symbol>
		<terminal_symbol>tilde</terminal_symbol>
		<terminal_symbol>lessthan</terminal_symbol>
		<terminal_symbol>greaterthan</terminal_symbol>
		<terminal_symbol>arrow</terminal_symbol>
		<terminal_symbol>arroba</terminal_symbol>
		<terminal_symbol>openquestionmark</terminal_symbol>
		<terminal_symbol>closequestionmark</terminal_symbol>
	</terminal_symbols>
	<productions>
		<PLANTUML_ACTIVITY_DIAGRAM>
			<derivation>
				<noterminal>HEADER</noterminal>
				<noterminal>PATHS</noterminal>
				<noterminal>FOOTER</noterminal>
			</derivation>
		</PLANTUML_ACTIVITY_DIAGRAM>
		<HEADER>
			<derivation>
				<terminal>arroba</terminal>
				<terminal>startuml</terminal>
				<terminal>start</terminal>
			</derivation>
		</HEADER>
		<FOOTER>
			<derivation>
				<terminal>stop</terminal>
				<terminal>arroba</terminal>
				<terminal>enduml</terminal>
			</derivation>
		</FOOTER>
		<PATHS>
			<derivation>
				<noterminal>IF_PATH</noterminal>
				<noterminal>PATHS</noterminal>
			</derivation>
			<derivation>
				<noterminal>IF_PATH</noterminal>
			</derivation>
		</PATHS>
		<IF_PATH>
			<derivation>
				<noterminal>IF</noterminal>
				<noterminal>ELSE_IF</noterminal>
				<noterminal>ELSE</noterminal>
			</derivation>
			<derivation>
				<noterminal>IF</noterminal>
				<noterminal>ELSE</noterminal>
			</derivation>
		</IF_PATH>
		<IF>
			<derivation>
				<terminal>if</terminal>
				<noterminal>CONDITION</noterminal>
				<terminal>then</terminal>
				<terminal>openparentesis</terminal>
				<terminal>identifier</terminal>
				<terminal>closeparentesis</terminal>
				<noterminal>PATH_CONTENT</noterminal>
			</derivation>
		</IF>
		<ELSE_IF>
			<derivation>
				<terminal>elseif</terminal>
				<noterminal>CONDITION</noterminal>
				<terminal>then</terminal>
				<terminal>openparentesis</terminal>
				<terminal>identifier</terminal>
				<terminal>closeparentesis</terminal>
				<noterminal>PATH_CONTENT</noterminal>
			</derivation>
		</ELSE_IF>
		<ELSE_IFS>
			<derivation>
				<noterminal>ELSE_IF</noterminal>
				<noterminal>ELSE_IFS</noterminal>
			</derivation>
			<derivation>
				<noterminal>ELSE_IF</noterminal>
			</derivation>
		</ELSE_IFS>
		<ELSE>
			<derivation>
				<terminal>else</terminal>
				<terminal>openparentesis</terminal>
				<terminal>identifier</terminal>
				<terminal>closeparentesis</terminal>
				<noterminal>PATH_CONTENT</noterminal>
				<terminal>endif</terminal>
			</derivation>
		</ELSE>
		<EXPRESSION>
			<derivation>
				<terminal>identifier</terminal>
			</derivation>
		</EXPRESSION>
		<CONDITION>
			<derivation>
				<terminal>openparentesis</terminal>
				<terminal>identifier</terminal>
				<noterminal>COMPARATOR</noterminal>
				<terminal>identifier</terminal>
				<terminal>closeparentesis</terminal>
			</derivation>
		</CONDITION>
		<COMPARATOR>
			<derivation>
				<terminal>lessthan</terminal>
			</derivation>
			<derivation>
				<terminal>greaterthan</terminal>
			</derivation>			
		</COMPARATOR>
		<PATH_CONTENT>
			<derivation>
				<noterminal>PATHS</noterminal>
			</derivation>
			<derivation>
				<noterminal>OUTPUT_SETTING</noterminal>
			</derivation>
		</PATH_CONTENT>
		<OUTPUT_SETTING>
			<derivation>
				<terminal>colon</terminal>
				<noterminal>EXPRESSION</noterminal>
				<terminal>semicolon</terminal>
			</derivation>
		</OUTPUT_SETTING>
	</productions>
</context_free_grammar>